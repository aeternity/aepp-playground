{
  "name": "aepp-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "faucet": "node -r esm ./faucet/index.js",
    "example": "cd examples && npm run serve",
    "node:up": "docker-compose up -d",
    "node:down": "docker-compose down -v",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@aeternity/aepp-components": "git+https://github.com/aeternity/aepp-components.git#develop",
    "@aeternity/aepp-sdk": "^2.0.0",
    "axios": "^0.18.0",
    "lodash.debounce": "^4.0.8",
    "lodash.defer": "^4.1.0",
    "monaco-editor": "^0.15.6",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "cors": "^2.8.5",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "esm": "^3.2.4",
    "express": "^4.16.4",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "raw-loader": "^1.0.0",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^0.7.4",
    "vue-template-compiler": "^2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
